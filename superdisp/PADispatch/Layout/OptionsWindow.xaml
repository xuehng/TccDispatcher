<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="renstech.NET.SupernovaDispatcher.PADispatch.OptionsWindow"
	x:Name="Window"
	Title="选项"
	Width="709" Height="575" ResizeMode="NoResize" ShowInTaskbar="True" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">

	<Grid x:Name="LayoutRoot" Margin="8">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<TabControl>
			<TabItem Header="账户设置" FontSize="16" FontFamily="/SupernovaDispatcher;component/Fonts/#FZYaoTi">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition/>
						<ColumnDefinition/>
					</Grid.ColumnDefinitions>
					<Grid/>
					<Grid Grid.Column="1"/>
				</Grid>
			</TabItem>
			<TabItem Header="TabItem">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
		</TabControl>
		<StackPanel Margin="0,10,0,0" Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
			<Button Content="确定" Width="150" Height="40" Margin="0,0,10,0" FontSize="16" FontFamily="/SupernovaDispatcher;component/Fonts/#FZYaoTi"/>
			<Button Content="取消" Width="150" Height="40" IsCancel="True" IsDefault="True" FontFamily="/SupernovaDispatcher;component/Fonts/#FZYaoTi" FontSize="16"/>
		</StackPanel>
	</Grid>
</Window>